        },    
        {
            "tag": "71",
            "name": "Issuer Script Template 2",
            "description": "Contains proprietary issuer data for transmission to the ICC after the second GENERATE AC command"
        },
        {
            "tag": "5F2D",
            "name": "Language Preference",
