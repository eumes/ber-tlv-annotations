        },
        {
            "tag": "5F24",
            "name": "Application Expiration Date",
            "description": "Date after which application expires",
            "format": "YYMMDD"         
        },                                        
        {
            "tag": "9F06",
            "name": "Application Identifier (AID) – terminal",
