                },
                {
                    "bitmask": "00 07 00",
                    "name": "RFU"
                },


                {
                    "bitmask": "00 00 80",
                    "name": "SDA"
                },
                {
                    "bitmask": "00 00 40",
                    "name": "DDA"
                },
                {
                    "bitmask": "00 00 20",
                    "name": "Card capture"
                },
                {
                    "bitmask": "00 00 10",
                    "name": "RFU"
                },
                {
                    "bitmask": "00 00 08",
                    "name": "CDA"
                },
                {
                    "bitmask": "00 00 07",
                    "name": "RFU"
                }
            ]
        },
        {
            "tag": "9F1A",
            "name": "Terminal Country Code",
            "description": "Indicates the country of the terminal, represented according to ISO 3166",
